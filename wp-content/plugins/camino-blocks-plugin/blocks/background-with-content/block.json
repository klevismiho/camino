{
	"apiVersion": 2,
	"name": "camino-blocks-plugin/background-with-content",
	"version": "0.1.0",
	"title": "Camino - Background with content",
	"category": "widgets",
	"icon": "insert",
	"description": "Camino backgroubnd with inner content",
	"supports": {
		"html": false
	},
	"textdomain": "camino-blocks-plugin",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"title": {
			"type": "string",
			"selector": "h2",
			"default": "Lorem ipsum dolor sit"
		},
		"subtitle": {
			"type": "string",
			"default": "Welcome to Camino"
		},
		"content": {
			"type": "string",
			"default": "This is an editable text"
		},
		"link": {
			"type": "string"
		},
		"linklabel": {
			"type": "string"
		},
		"image": {
			"type": "object",
			"selector": "image-upload"
		},
		"fullBackground": {
			"type": "boolean"
		},
		"halfBackground": {
			"type": "boolean"
		},
		"halfTopBackground": {
			"type": "boolean"
		}
	}
}